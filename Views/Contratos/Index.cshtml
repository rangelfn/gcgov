@model IEnumerable<gcgov.Models.Contrato>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
     <a asp-action="Create" class="btn btn-primary">Cadastrar</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Extrato)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contratada)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Objeto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProcessoSei)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProtocoloDiof)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Valor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModLicitacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UgCodigo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Extrato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Contratada)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Objeto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProcessoSei)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProtocoloDiof)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Valor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModLicitacao.ModLicitacaoId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UgCodigo.UgCodigoId)
            </td>
            <td>
                <a asp-action="Edit" class="btn btn-primary" class="btn btn-primary" asp-route-id="@item.ContratoId" title="Editar"><i class="fas fa-pen-square mr-2"></i></a>
                <a asp-action="Details" class="btn btn-success" class="btn btn-success" asp-route-id="@item.ContratoId" title="Detalhar"><i class="fas fa-file-alt mr-2"></i></a>
                <a asp-action="Delete" class="btn btn-danger" class="btn btn-danger" asp-route-id="@item.ContratoId" title="Apagar"><i class="fas fa-trash mr-2"></i></a>
            </td>
        </tr>
}
    </tbody>
</table>
